{
    "manifest_version":2,
    "name":"onboard tool",
    "version":"1.0",
    "browser_action":{
        "default_title":"onboard",
        "default_popup":"dist/index.html"
    },
    "content_scripts":[
        {
           "matches": [
        "<all_urls>"
      ],
      "run_at":"document_idle",
      "js": ["node_modules/jquery/dist/jquery.min.js", "dist/main.js"] 
        }
    ]
}