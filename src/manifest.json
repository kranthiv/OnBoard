{
    "manifest_version":2,
    "name":"onboard tool",
    "version":"1.0",
    "browser_action":{
        "default_title":"onboard",
        "default_popup":"index.html"
    },
    "permissions": ["contextMenus"],
  "background": {
    "scripts": ["backgroundScript.js"]
  },
    "content_scripts":[
        {
           "matches": [
        "<all_urls>"
      ],
      "run_at":"document_idle",
      "js": ["contentScript.js"] 
        }
    ]
}